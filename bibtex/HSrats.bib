@article{Wen2012,
abstract = {Multiple genetic loci associated with obesity or body mass index (BMI) have been identified through genome-wide association studies conducted predominantly in populations of European ancestry. We performed a meta-analysis of associations between BMI and approximately 2.4 million SNPs in 27,715 east Asians, which was followed by in silico and de novo replication studies in 37,691 and 17,642 additional east Asians, respectively. We identified ten BMI-associated loci at genome-wide significance (P {\textless} 5.0 × 10(-8)), including seven previously identified loci (FTO, SEC16B, MC4R, GIPR-QPCTL, ADCY3-DNAJC27, BDNF and MAP2K5) and three novel loci in or near the CDKAL1, PCSK1 and GP2 genes. Three additional loci nearly reached the genome-wide significance threshold, including two previously identified loci in the GNPDA2 and TFAP2B genes and a newly identified signal near PAX6, all of which were associated with BMI with P {\textless} 5.0 × 10(-7). Findings from this study may shed light on new pathways involved in obesity and demonstrate the value of conducting genetic studies in non-European populations.},
author = {Wen, Wanqing and Cho, Yoon-Shin and Zheng, Wei and Dorajoo, Rajkumar and Kato, Norihiro and Qi, Lu and Chen, Chien-Hsiun and Delahanty, Ryan J and Okada, Yukinori and Tabara, Yasuharu and Gu, Dongfeng and Zhu, Dingliang and Haiman, Christopher A and Mo, Zengnan and Gao, Yu-Tang and Saw, Seang-Mei and Go, Min-Jin and Takeuchi, Fumihiko and Chang, Li-Ching and Kokubo, Yoshihiro and Liang, Jun and Hao, Mei and {Le Marchand}, Lo{\"{i}}c and Zhang, Yi and Hu, Yanling and Wong, Tien-Yin and Long, Jirong and Han, Bok-Ghee and Kubo, Michiaki and Yamamoto, Ken and Su, Mei-Hsin and Miki, Tetsuro and Henderson, Brian E and Song, Huaidong and Tan, Aihua and He, Jiang and Ng, Daniel P-K and Cai, Qiuyin and Tsunoda, Tatsuhiko and Tsai, Fuu-Jen and Iwai, Naoharu and Chen, Gary K and Shi, Jiajun and Xu, Jianfeng and Sim, Xueling and Xiang, Yong-Bing and Maeda, Shiro and Ong, Rick T H and Li, Chun and Nakamura, Yusuke and Aung, Tin and Kamatani, Naoyuki and Liu, Jian-Jun and Lu, Wei and Yokota, Mitsuhiro and Seielstad, Mark and Fann, Cathy S J and {Genetic Investigation of ANthropometric Traits (GIANT) Consortium} and Wu, Jer-Yuarn and Lee, Jong-Young and Hu, Frank B and Tanaka, Toshihiro and Tai, E Shyong and Shu, Xiao-Ou},
