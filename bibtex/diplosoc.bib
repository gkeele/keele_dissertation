title = {{Mapping mendelian factors underlying quantitative traits using RFLP linkage maps.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1203601{\&}tool=pmcentrez{\&}rendertype=abstract},
