keywords = {Arabidopsis,Arabidopsis: genetics,Bayes Theorem,Chromosome Mapping,Chromosome Mapping: methods,Founder Effect,Haplotypes,Models, Genetic,Monte Carlo Method,Quantitative Trait Loci,Quantitative Trait Loci: genetics},
month = {mar},
