pmid = {20048050},
title = {{Bayesian quantitative trait locus mapping using inferred haplotypes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2845350{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {184},
year = {2010}
}
