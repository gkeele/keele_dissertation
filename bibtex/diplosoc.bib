pmid = {28592651},
title = {{Identification of Nitrogen Consumption Genetic Variants in Yeast Through QTL Mapping and Bulk Segregant RNA-Seq Analyses}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28592651{\%}0Ahttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5473750},
