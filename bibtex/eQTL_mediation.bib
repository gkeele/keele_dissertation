isbn = {1476-4687 (Electronic) 0028-0836 (Linking)},
issn = {1476-4687},
journal = {Nature},
month = {jun},
number = {7608},
pages = {500--5},
pmid = {27309819},
publisher = {Nature Publishing Group},
title = {{Defining the consequences of genetic variation on a proteome-wide scale.}},
url = {http://dx.doi.org/10.1038/nature18270 http://www.nature.com/doifinder/10.1038/nature18270 http://www.ncbi.nlm.nih.gov/pubmed/27309819 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5292866},
volume = {534},
