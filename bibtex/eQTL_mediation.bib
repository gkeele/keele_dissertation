issn = {1460-2083},
journal = {Human molecular genetics},
month = {apr},
number = {8},
pages = {1444--1451},
pmid = {28165122},
title = {{Conditional eQTL analysis reveals allelic heterogeneity of gene expression.}},
url = {https://academic.oup.com/hmg/article-lookup/doi/10.1093/hmg/ddx043 http://www.ncbi.nlm.nih.gov/pubmed/28165122},
volume = {26},
year = {2017}
